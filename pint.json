{
	"preset": "laravel",
	"rules": {
		"phpdoc_align": {
			"align": "vertical",
			"spacing": 2
		},
		"concat_space": {
			"spacing": "one"
		},
		"no_superfluous_phpdoc_tags": {
			"allow_mixed": true,
			"allow_unused_params": false,
			"remove_inheritdoc": false
		},
		"ordered_imports": {
			"sort_algorithm": "alpha"
		},
		"class_attributes_separation": {
			"elements": {
				"method": "one",
				"trait_import": "none",
				"case": "none"
			}
		}
	}
}